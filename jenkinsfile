@Library('common-templates')_

pipeline {
    agent any
    
    stages {
        stage("Clone Git Repo") {
            steps {
                script {
                    echo "Hello"
                    checkoutCode("https://github.com/octocat/Hello-World","master")
                }
            }
    
        }
        stage("Dummy Stage") {
            steps {
                script {
                    def sayHello = sayHello('yyj')
                }
            }

        }
    }
    post {
        always {
                sh 'rm -rf ${WORKSPACE}/*'     
            }
            
        }
    }
