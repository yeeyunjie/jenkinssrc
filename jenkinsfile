@Library('common-templates')_

pipeline {
    agent any
    
    stages {
        stage("Clone Git Repo") {
            steps {
                script {
                    echo "Hello"
                    checkoutCode("https://github.com/octocat/Hello-World","master")
                }
            }
    
        }
        stage("Test") {
            steps {
                script {
                    sayHello(name: "yyj", dayOfWeek: "Thursday")
                }
            }

        }
    }
    post {
        always {
                deleteDir()   
            }
            
        }
    }
